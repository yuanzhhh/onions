{
    "name": "onions",
    "version": "4.0.1",
    "description": "",
    "main": "./dist/onions.min.js",
    "module": "./lib/index.js",
    "types": "./lib/index.d.ts",
    "files": [
        "lib",
        "src",
        "dist"
    ],
    "scripts": {
        "prepare": "npm run clean && npm run lint && npm run test && npm run tsc && npm run build",
        "build": "npm run build:develop && npm run build:pro",
        "build:develop": "cross-env  NODE_ENV=development webpack",
        "build:pro": "cross-env NODE_ENV=production webpack",
        "tsc": "tsc -m es6 --outDir lib",
        "lint": "eslint ./src/index.ts",
        "lint-fix": "eslint ./src/index.ts --fix",
        "clean": "rimraf dist lib",
        "test": "jest"
    },
    "prettier": {
        "arrowParens": "always",
        "proseWrap": "always",
        "singleQuote": true,
        "trailingComma": "all"
    },
    "repository": {
        "type": "git",
        "url": "git+https://github.com/yuanzhhh/onions.git"
    },
    "keywords": [
        "onions",
        "middleware"
    ],
    "author": "evan yuanzh <evan.yuanzh@gmail.com>",
    "license": "ISC",
    "bugs": {
        "url": "https://github.com/yuanzhhh/onions/issues"
    },
    "homepage": "https://github.com/yuanzhhh/onions#readme",
    "devDependencies": {
        "@babel/cli": "^7.8.4",
        "@babel/core": "^7.9.6",
        "@babel/plugin-transform-runtime": "^7.9.6",
        "@babel/preset-env": "^7.9.6",
        "@babel/preset-typescript": "^7.9.0",
        "@babel/runtime-corejs2": "^7.9.6",
        "@babel/types": "^7.9.6",
        "@types/jest": "^25.2.2",
        "@types/node": "^13.9.3",
        "@typescript-eslint/eslint-plugin": "^2.33.0",
        "@typescript-eslint/parser": "^2.33.0",
        "babel-jest": "^26.0.1",
        "babel-loader": "^8.1.0",
        "core-js": "^2.6.11",
        "cross-env": "^7.0.2",
        "eslint": "^7.0.0",
        "eslint-config-airbnb": "^18.1.0",
        "eslint-config-prettier": "^6.11.0",
        "eslint-plugin-import": "^2.20.2",
        "eslint-plugin-jsx-a11y": "^6.2.3",
        "eslint-plugin-prettier": "^3.1.3",
        "eslint-plugin-react": "^7.20.0",
        "jest": "^26.0.1",
        "prettier": "^2.0.5",
        "rimraf": "^3.0.2",
        "terser-webpack-plugin": "^3.0.1",
        "ts-jest": "^25.5.1",
        "typescript": "^3.9.2",
        "webpack": "^4.43.0",
        "webpack-cli": "^3.3.11"
    },
    "jest": {
        "testRegex": "(/test/.*\\.spec\\.[tj]s)$",
        "coverageProvider": "v8"
    },
    "dependencies": {}
}
