{
    "presets": [
        ["@babel/preset-env", {
            "useBuiltIns": "usage",
            "modules": false
        }]
    ],
    "plugins": [
        [
            "@babel/plugin-transform-runtime", {
                "helpers": false,
                "regenerator": false,
                "useESModules": false,
                "corejs": 2
            }
        ]
    ],
    "comments": false
}
