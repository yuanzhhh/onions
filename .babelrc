{
    "presets": [
        "@babel/typescript",
        [
            "@babel/preset-env",
            {
                "useBuiltIns": "usage",
                "modules": false,
                "targets": {
                    "browsers": [
                        "ie >= 11"
                    ]
                }
            }]
    ],
    "plugins": [
        [
            "@babel/plugin-transform-runtime", {
                "helpers": false,
                "regenerator": false,
                "useESModules": false,
                "corejs": 2
            }
        ]
    ],
    "comments": false
}
